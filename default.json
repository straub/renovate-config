{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackagePatterns": ["(^|\\/)traefik$"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/containous/traefik"
    },
    {
      "matchPackageNames": ["authelia/authelia"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/authelia/authelia"
    },
    {
      "matchPackageNames": ["grafana/grafana"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/grafana/grafana"
    },
    {
      "matchPackagePrefixes": ["haveagitgat/tdarr"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/HaveAGitGat/Tdarr"
    },
    {
      "matchPackageNames": ["telegraf"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/influxdata/telegraf"
    },
    {
      "matchPackageNames": ["instantlinux/nut-upsd"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/instantlinux/docker-tools/tree/main/images/nut-upsd"
    },
    {
      "matchPackageNames": ["mazzolino/restic"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/djmaze/resticker/"
    },
    {
      "matchPackageNames": ["containrrr/watchtower"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/containrrr/watchtower/"
    },
    {
      "matchPackageNames": ["ghcr.io/esphome/esphome"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/esphome/esphome/"
    },
    {
      "matchPackageNames": ["ghcr.io/mealie-recipes/mealie"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/mealie-recipes/mealie/"
    },
    {
      "matchPackageNames": ["prom/prometheus"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/prometheus/prometheus/"
    }
  ]
}
